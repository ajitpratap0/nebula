# Nebula Environment Variables Example
# Copy this file to .env and fill in your values

# General Configuration
LOG_LEVEL=info
ENVIRONMENT=development

# Source: Google Ads
GOOGLE_ADS_DEVELOPER_TOKEN=your_developer_token
GOOGLE_ADS_CLIENT_ID=your_client_id
GOOGLE_ADS_CLIENT_SECRET=your_client_secret
GOOGLE_ADS_REFRESH_TOKEN=your_refresh_token
GOOGLE_ADS_CUSTOMER_ID=123-456-7890

# Source: Meta Ads
META_ADS_ACCESS_TOKEN=your_access_token
META_ADS_ACCOUNT_ID=act_123456789

# Source: LinkedIn
LINKEDIN_ACCESS_TOKEN=your_access_token
LINKEDIN_ACCOUNT_ID=123456789

# Destination: Snowflake
SNOWFLAKE_ACCOUNT=your_account
SNOWFLAKE_USER=your_user
SNOWFLAKE_PASSWORD=your_password
SNOWFLAKE_WAREHOUSE=your_warehouse
SNOWFLAKE_DATABASE=your_database
SNOWFLAKE_SCHEMA=your_schema
SNOWFLAKE_ROLE=your_role

# Destination: BigQuery
BIGQUERY_PROJECT_ID=your_project_id
BIGQUERY_DATASET_ID=your_dataset_id
BIGQUERY_CREDENTIALS_JSON=path/to/credentials.json

# Destination: S3
AWS_ACCESS_KEY_ID=your_access_key
AWS_SECRET_ACCESS_KEY=your_secret_key
AWS_REGION=us-east-1
S3_BUCKET=your_bucket_name

# Monitoring
PROMETHEUS_PORT=9090
METRICS_ENABLED=true